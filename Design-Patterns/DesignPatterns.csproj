<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>DesignPatterns</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\DesignPatterns.Behavioral\DesignPatterns.Behavioral.csproj" />
    <ProjectReference Include="..\DesignPatterns.Creational\DesignPatterns.Creational.csproj" />
    <ProjectReference Include="..\DesignPatterns.GRASP\DesignPatterns.GRASP.csproj" />
    <ProjectReference Include="..\DesignPatterns.SOLID\DesignPatterns.SOLID.csproj" />
    <ProjectReference Include="..\DesignPatterns.Structural\DesignPatterns.Structural.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
